<form {{on "submit" this.submitForm}}>
    <div class="container">
        <div class="form-group">
            <Input class="form-control"  @type="text" @value={{this.email}} placeholder="Email*"/>
            {{!-- <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> --}}
        </div>
        <div class="form-group">
            <Input @type="password" class="form-control" @value={{this.password}} placeholder="Password*"/>
            {{!-- <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> --}}
        </div>
        <button type="submit" class="btn btn-primary" disabled={{this.disableSubmit}}>Login</button>
    </div>
</form>